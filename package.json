{
  "name": "workplace-spectrum",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "sideEffects": false,
  "scripts": {
    "prepare": "husky",
    "dev": "vite",
    "dev:dev": "vite --mode develop",
    "dev:mock": "vite --mode mock",
    "build": "run-p type-check \"build-only {@}\"",
    "build-only": "vite build",
    "build-analyze": "vite build --mode analyze",
    "preview": "vite preview",
    "test:unit": "vitest",
    "type-check": "vue-tsc --build",
    "lint": "eslint",
    "lint:fix": "eslint . --fix",
    "style-lint": "npx stylelint \"src/**/*.css\"",
    "style-lint:fix": "npx stylelint \"src/**/*.css\" --fix",
    "format": "prettier --write src/",
    "generate-report": "npm outdated --json | node -e \"process.stdin.on('data', data => { const json = JSON.parse(data.toString()); const ignoreList = ['zod']; const filtered = Object.fromEntries(Object.entries(json).filter(([pkg]) => !ignoreList.includes(pkg)));const cleanJson = JSON.stringify(filtered, null, 2); require('fs').writeFileSync('outdated-report.json', cleanJson); });\"",
    "update": "npm update --depth=9999"
  },
  "dependencies": {
    "@primeuix/themes": "1.2.3",
    "@tanstack/vue-query": "5.83.1",
    "@types/node": "^24.2.0",
    "@vee-validate/zod": "4.15.1",
    "crypto-pro": "2.5.2",
    "husky": "^9.1.7",
    "ky": "1.8.2",
    "luxon": "3.7.1",
    "pinia": "3.0.3",
    "primeicons": "7.0.0",
    "primevue": "4.3.7",
    "uuid": "^11.1.0",
    "vee-validate": "4.15.1",
    "vite-plugin-vue-devtools": "^8.0.0",
    "vue": "3.5.18",
    "vue-i18n": "11.1.11",
    "vue-router": "4.5.1",
    "vue-tsc": "^3.0.5",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "5.2.2",
    "@tsconfig/node22": "22.0.2",
    "@types/jsdom": "21.1.7",
    "@types/luxon": "^3.7.1",
    "@types/node": "24.2.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/eslint-plugin": "1.3.4",
    "@vue/eslint-config-prettier": "10.2.0",
    "@vue/eslint-config-typescript": "14.6.0",
    "@vue/test-utils": "2.4.6",
    "@vue/tsconfig": "0.7.0",
    "eslint": "9.32.0",
    "eslint-plugin-vue": "10.4.0",
    "jiti": "2.5.1",
    "jsdom": "26.1.0",
    "lint-staged": "^16.1.4",
    "npm-run-all2": "8.0.4",
    "prettier": "3.6.2",
    "rollup-plugin-visualizer": "6.0.3",
    "sass-embedded": "^1.90.0",
    "stylelint": "^16.23.0",
    "stylelint-config-standard": "^39.0.0",
    "typescript": "5.9.2",
    "vite": "7.0.6",
    "vite-svg-loader": "5.1.0",
    "vitest": "3.2.4"
  }
}
